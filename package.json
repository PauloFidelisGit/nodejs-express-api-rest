{
  "name": "nodejs-express-api-rest",
  "version": "1.0.0",
  "main": "server.js",
  "private": true,
  "scripts": {
    "test-ui": "vitest --ui",
    "test": "vitest --reporter verbose",
    "coverage": "vitest run --coverage",
    "build": "rimraf ./build && tsc",
    "reset": "rimraf ./package-lock.json && rimraf ./node_modules && yarn upgrade --latest && yarn install",
    "start:dev": "npx tsx watch ./src/server.ts",
    "start:prod": "yarn build && node build/server.js"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "9.0.1",
    "@types/luxon": "^3.2.0",
    "@types/method-override": "^0.0.32",
    "@types/node": "^18.15.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "supertest": "^6.3.3",
    "typescript": "^4.7.4",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "^6.7.0",
    "prisma-database": "file:../prisma-database",
    "helmet": "^6.0.1",
    "jsonwebtoken": "9.0.0",
    "luxon": "^3.3.0",
    "tsx": "^3.12.4",
    "uuidv4": "^6.2.13",
    "zod": "^3.21.4"
  }
}
